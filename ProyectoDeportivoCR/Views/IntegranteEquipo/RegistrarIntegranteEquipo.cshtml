@model IntegranteEquipoModel

<form asp-controller="IntegranteEquipo" asp-action="RegistrarIntegranteEquipo" method="post">
    <input type="hidden" asp-for="EquipoId" />

    <div class="form-group">
        <label asp-for="Cedula">Cédula</label>
        <input asp-for="Cedula" class="form-control" required />
    </div>

    <div class="form-group">
        <label asp-for="Rol">Rol</label>
        <input asp-for="Rol" class="form-control" required />
    </div>

    <button type="submit" class="btn btn-success">Registrar</button>
</form>
